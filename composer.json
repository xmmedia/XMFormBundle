{
    "name": "xm/form-bundle",
    "description": "A few helpers to make processing (creating, saving, errors, etc) forms simpler.",
    "type": "symfony-bundle",
    "homepage": "https://github.com/xmmedia/XMFormBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Hein",
            "email": "dhein@xmmedia.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/form": "~3.0",
        "doctrine/orm": "^2.5",
        "xm/flash-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "symfony/var-dumper": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "mockery/mockery": "^0.9"
    },
    "autoload": {
        "psr-4": { "XM\\FormBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
